{
	"name": "4dn-microscopy-metadata-tool",
	"version": "0.1.0",
	"main": "./dist/MicroscopyMetadataTool.min.js",
	"unpkg": "./dist/MicroscopyMetadataTool.min.js",
	"module": "./dist/MicroscopyMetadataTool.dev.js",
	"private": true,
	"publishConfig": {
		"access": "restricted"
	},
	"dependencies": {
		"bfj": "6.1.1",
		"file-loader": "3.0.1",
		"identity-obj-proxy": "3.0.0",
		"imagemin": "^6.1.0",
		"img-loader": "^3.0.1",
		"jquery": "^3.4.1",
		"jsonschema": "^1.2.4",
		"popper.js": "^1.15.0",
		"rc-tabs": "^9.6.4",
		"react-app-polyfill": "^1.0.1",
		"react-bootstrap": "^1.0.0-beta.8",
		"react-collapsible": "^2.6.0",
		"react-drag-drop-container": "^6.1.1",
		"react-dropzone": "^10.1.5",
		"react-jsonschema-form": "^1.5.0",
		"react-resizable": "^1.8.0",
		"react-simple-animate": "^3.2.0",
		"resolve": "1.11.0",
		"url-loader": "1.1.2",
		"uuid": "^3.3.2"
	},
	"scripts": {
		"test": "node scripts/test.js",
		"build": "gulp build",
		"dev": "gulp dev",
		"build-all": "gulp build-dev && gulp build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.6",
		"babel-preset-react-app": "^9.0.0",
		"case-sensitive-paths-webpack-plugin": "2.2.0",
		"css-loader": "2.1.1",
		"eslint": "5.16.0",
		"eslint-loader": "2.1.2",
		"eslint-plugin-flowtype": "3.9.1",
		"eslint-plugin-import": "2.17.2",
		"eslint-plugin-jsx-a11y": "6.2.1",
		"eslint-plugin-react": "7.13.0",
		"fancy-log": "^1.3.3",
		"gulp": "^4.0.2",
		"jest": "24.8.0",
		"jest-pnp-resolver": "1.2.1",
		"jest-resolve": "24.8.0",
		"jest-watch-typeahead": "^0.3.1",
		"plugin-error": "^1.0.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"style-loader": "0.23.1",
		"terser-webpack-plugin": "1.3.0",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2",
		"workbox-webpack-plugin": "4.3.1"
	},
	"peerDependencies": {
		"react": ">=16.8.6",
		"react-dom": ">=16.8.6"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	}
}
